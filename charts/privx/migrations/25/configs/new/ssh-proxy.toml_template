servicename = "ssh-proxy"

listen_port = 8880

# Keyvault credentials.
keyvault_id = "@SSHPROXY_ID@"
keyvault_secret = "@SSHPROXY_SECRET@"

[tls]
# Microservice https server settings
enable_https_server = true
certificate = "/opt/privx/cert/ssh-proxy.crt"
private_key = "/opt/privx/cert/ssh-proxy.key"
