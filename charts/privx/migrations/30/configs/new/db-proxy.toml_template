servicename = "db-proxy"

# Default port for db proxy
listen_port = 8983

# db-proxy keyvault credentials.
keyvault_id = "@DBPROXY_ID@"
keyvault_secret = "@DBPROXY_SECRET@"

[tls]
# Microservice https server settings
enable_https_server = true
certificate = "/opt/privx/cert/db-proxy.crt"
private_key = "/opt/privx/cert/db-proxy.key"

[db]
# DB data version number
# Do not modify
data_version = "1"
